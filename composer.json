{
  "name": "izzai/one-php",
  "version": "1.0.0",
  "description": "One package for PHP projects.",
  "type": "library",
  "homepage": "https://www.izz.ai/",
  "license": "SEE LICENSE.md",
  "authors": [
    {
      "name": "Adarsh Mohan",
      "email": "adarsh@izz.ai"
    }
  ],
  "keywords": [
    "one",
    "izz",
    "izz one",
    "nexus",
    "php",
    "api",
    "sdk",
    "izz.ONE"
  ],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Izzai\\One\\": "src/"
    },
    "classmap": [
      "src/Types/One.php",
      "src/Types/Chat.php",
      "src/Types/Auth.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
